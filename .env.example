# Environment variables for Cloudflare R2
# Copy this file to .env and fill in your actual values

# Your Cloudflare Account ID (found in the R2 dashboard)
R2_ACCOUNT_ID=your_account_id_here

# R2 Endpoint URL (replace with your account ID)
R2_ENDPOINT_URL=https://your_account_id_here.r2.cloudflarestorage.com

# R2 API Token credentials (create in Cloudflare dashboard)
R2_ACCESS_KEY_ID=your_access_key_id_here
R2_SECRET_ACCESS_KEY=your_secret_access_key_here

# Optional: Custom domain for your R2 bucket
R2_CUSTOM_DOMAIN=currency-icons.yourdomain.com